<?php

namespace EntitiesBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Tchat
 */
class Tchat
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $nickname;

    /**
     * @var string
     */
    private $contentMsg;

    /**
     * @var \DateTime
     */
    private $dateMsg;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set nickname
     *
     * @param string $nickname
     * @return Tchat
     */
    public function setNickname($nickname)
    {
        $this->nickname = $nickname;

        return $this;
    }

    /**
     * Get nickname
     *
     * @return string 
     */
    public function getNickname()
    {
        return $this->nickname;
    }

    /**
     * Set contentMsg
     *
     * @param string $contentMsg
     * @return Tchat
     */
    public function setContentMsg($contentMsg)
    {
        $this->contentMsg = $contentMsg;

        return $this;
    }

    /**
     * Get contentMsg
     *
     * @return string 
     */
    public function getContentMsg()
    {
        return $this->contentMsg;
    }

    /**
     * Set dateMsg
     *
     * @param \DateTime $dateMsg
     * @return Tchat
     */
    public function setDateMsg($dateMsg)
    {
        $this->dateMsg = $dateMsg;

        return $this;
    }

    /**
     * Get dateMsg
     *
     * @return \DateTime 
     */
    public function getDateMsg()
    {
        return $this->dateMsg;
    }
}
